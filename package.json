    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vitest": "^0.5.4",
    "gts": "^5.3.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
